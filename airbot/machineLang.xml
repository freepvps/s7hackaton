<row value="хуйн" simple_response="Фу, как грубо!"/>
<row value="хуйрик" simple_response="Фу, как грубо!"/>
<row value="хуяка" simple_response="Фу, как грубо!"/>
<row value="хуякнут" simple_response="Фу, как грубо!"/>
<row value="целк" simple_response="Фу, как грубо!"/>
<row value="шлюх" simple_response="Фу, как грубо!"/>
    </table>
  </tables>
  
  <processing>
    <register name="lower-case" /> <!-- value = value.tolower() -->
    <register name="integer-export" /> <!--if (value is int) integer = int(value)-->
    <register name="ticket-export" />
    <register name="table-export" table="export" />
  </processing>
  
  <mapping>
    <map><target h="lower-case" /></map>
    <map><target h="integer-export" /></map>
    <map><target h="ticket-export" /></map>
    <map><target h="table-export" /></map>
    <map>
      <seq t-city="spb">
        <target value="санкт" />
        <target value="петербург" />
      </seq>
    </map>
    <map>
      <seq>
        <target yes="no" />
        <target value="дорог" cost="high" t-cost="low" />
      </seq>
    </map>
    <map>
      <seq>
        <target city="*" t-cityFrom="true"/>
        <target s-min="0" s-max="3" />
        <target city="*" t-cityTo="true"/>
      </seq>
      <seq>
        <target base="from" />
        <target city="*" t-cityFrom="true"/>
      </seq>
      <seq>
        <target base="to" />
        <target city="*" t-cityTo="true"/>
      </seq>
    </map>
    <map>
      <seq t-searchTicket="true">
        <target buy="true" />
        <target value="*" s-min="0" s-max="10" />
        <target flight="true" />
      </seq>
      <seq t-searchTicket="true">
        <target ticket="true" />
        <target value="*" s-min="0" s-max="10" />
        <target city="*" />
      </seq>
      <seq t-searchTicket="true">
        <target want="true" />
        <target value="*" s-min="0" s-max="10" />
        <target city="*" />
      </seq>
      <seq t-searchTicket="true">
        <target flight="true" />
        <target value="*" s-min="0" s-max="10" />
        <target city="*" />
      </seq>
      <seq t-searchTicket="true">
        <target search="true" />
        <target value="*" s-min="0" s-max="10" />
        <target flight="true" />
      </seq>
    </map>
    <map>
      <target base="hi" t-simple_response="Привет!:) Есть какие-то вопросы?" />
        <seq>
            <target value="как" t-simple_response="Нормально все :)"/>
            <target value="дел"/>

        </seq>
        <seq>
            <target value="ест" />
            <target value="два" t-simple_response="Возьму пики точеные..."/>
            <target value="стул"/>
        </seq>
        <seq>
            <target value="мор" t-simple_response="В каждом море есть островок, в каждом лесу полянка"/>
            <target value="говн"/>
        </seq>
    </map>
    <map>
        <seq>
          <target subscribe="*" t-subscribe="true" />
          <target s-min="0" s-max="10" />
          <target value="мо" s-min="0"/>
          <target value="о" s-min="0"/>
          <target value="рейс"/>
        </seq>
    </map>

    <map>
        <seq t-support="true">
            <target value="нужн"/>
            <target value="помощ"/>
        </seq>
        <seq t-where="true">
            <target value="куд"/>
            <target value="идт"/>
        </seq>
    </map>

    <map>
      <seq t-callme="true">
        <target value="мен" />
        <target value="называ"/>
      </seq>
      <seq t-callme="true">
        <target value="называ"/>
        <target value="мен" />
      </seq>
      <seq t-callme="true">
          <target value="зов"/>
          <target value="мен" />
      </seq>
    </map>

    <map>
      <seq>
        <target base="can" />
        <target s-min="0" s-max="100" s-handle="false" />
        <target base="give" />
        <seq s-min="1" s-max="100" s-maximize="true" >
          <target s-min="0" s-max="100" s-handle="false" />
          <target bagage="*" t-bagageCheck="true" />
        </seq>
      </seq>
    </map>
  </mapping>
</db>
